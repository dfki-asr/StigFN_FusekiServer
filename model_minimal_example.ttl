                       [ a             ex:negFeedback ;
                                       st:created    "2021-04-20T12:53:36.705+02:00"^^xsd:dateTime ;
                                       st:decayRate  "0.005"^^xsd:double ;
                                       st:level      500
                       ].


en:stigma_diff  a     st:Stigma , ex:transportStigma ;
        st:created    "2021-03-26T15:56:00+01:00"^^xsd:dateTime ;
        st:decayRate  "0.0005"^^xsd:double ;
        st:level      "100.0"^^xsd:double .

topos:5  a            st:Topos ;
        pos:xPos       2 ;
        pos:yPos       2 ;
        st:adjacentTo  topos:15 .

topos:15  a            st:Topos ;
        pos:xPos       2 ;
        pos:yPos       2 ;
        st:adjacentTo  topos:25 , topos:14 , topos:16 , topos:5 ;
        st:carries     [ a          st:diffusionTrace ;
                         st:level   "1"^^xsd:double ;
                         st:source  en:stigma_diff
                       ] .

topos:16  a            st:Topos ;
        pos:xPos       2 ;
        pos:yPos       2 ;
        st:adjacentTo  topos:26 , topos:17 , topos:15 , topos:6 .

en:stigma_evap  a     st:Stigma , ex:negFeedback ;
        st:created    "2021-04-20T10:40:00+01:00"^^xsd:dateTime ;
        st:decayRate  "0.0005"^^xsd:double ;
        st:level      "100.0"^^xsd:double .

