ex:medium  a    st:Medium .

ex:diffusionLaw  a  st:Law ;
        st:affects  ex:medium ;
        st:rule     law:diffusion .

topos:25  a            st:Topos ;
        pos:xPos       2 ;
        pos:yPos       1 ;
        st:adjacentTo  topos:24 , topos:26 , topos:35 , topos:15 ;
        st:carries     [ a          st:diffusionTrace ;
                         st:level   "2"^^xsd:double ;
                         st:source  en:stigma_diff
                       ] .

topos:24  a            st:Topos ;
        pos:xPos       2 ;
        pos:yPos       3 ;
        st:adjacentTo  topos:25 , topos:34 , topos:23 , topos:14 ;
        st:carries     en:stigma_diff ;
        st:carries     [ a          st:diffusionTrace ;
                         st:level   "3"^^xsd:double ;
                         st:source  en:stigma_diff
                       ],
                       [ a             ex:negFeedback ;
                                                               st:created    "2021-04-20T12:53:36.705+02:00"^^xsd:dateTime ;
                                                               st:decayRate  "0.5"^^xsd:double ;
                                                               st:level      1500
                       ].

topos:23  a            st:Topos ;
        pos:xPos       2 ;
        pos:yPos       2 ;
